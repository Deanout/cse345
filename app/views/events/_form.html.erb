<%= simple_form_for(@event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :body %>
    <%= f.input :event_date %>
    <%= f.input :event_time %>
    <%= f.input :enroll %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
